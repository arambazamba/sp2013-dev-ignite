<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="MailApp">
  <Id>b2c6573f-2677-4de1-b961-b574b6e4eb5f</Id>
  <Version>1.0</Version>
  <ProviderName>administrator</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="TwitterMailApp" />
  <Description DefaultValue="TwitterMailApp Description"/>
  <Capabilities>
    <Capability Name="Mailbox" />
  </Capabilities>
  <DesktopSettings>
    <SourceLocation DefaultValue="~remoteAppUrl/Pages/TwitterMailApp.html" />
    <RequestedHeight>250</RequestedHeight>
  </DesktopSettings>
  <Permissions>ReadItem</Permissions>
    <Rule xsi:type="RuleCollection" Mode="And">
      <Rule xsi:type="RuleCollection" Mode="And">
      <Rule xsi:type="ItemIs" ItemType="Message" />
      <Rule xsi:type="ItemHasRegularExpressionMatch" RegExName="ScreenName" RegExValue="sharepoint|spconf|andrewconnell|kaevans" PropertyName="BodyAsPlaintext" IgnoreCase="true" />
    </Rule>
    </Rule>
</OfficeApp>
